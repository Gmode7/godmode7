DATABASE_URL="file:./prisma/dev.db"
PORT=3000
NODE_ENV=development

# CORS (comma-separated origins, empty = allow all in dev)
CORS_ORIGINS=

# Dev bypass key (set to a seeded API key plaintext for local dev)
DEV_API_KEY=

# Redis (optional — falls back to in-memory if not set)
REDIS_URL=

# ═══════════════════════════════════════════════════════════════════════════════
# LLM Provider Keys
# ═══════════════════════════════════════════════════════════════════════════════
#
# FOR FREE TIER (RECOMMENDED FOR TESTING):
# Get a free OpenRouter API key at https://openrouter.ai/keys
# This gives you access to free models: Gemma 3, Llama 3.3, Hermes 3 405B
#
# FOR PAID TIER:
# You can also configure paid providers (OpenAI, Kimi, Claude)
# ═══════════════════════════════════════════════════════════════════════════════

# OpenRouter (FREE models - see README for setup)
# Get your key: https://openrouter.ai/keys
OPENROUTER_API_KEY=

# OpenAI (paid) - sk-...
OPENAI_API_KEY=

# Kimi (paid)
KIMI_API_KEY=

# Anthropic/Claude (paid) - sk-ant-...
ANTHROPIC_API_KEY=
